# Letta server environment variables for development
# Copy to server.env and adjust as needed

# Server configuration
MEMGPT_SERVER_PASS=test_password
MEMGPT_SERVER_URL=http://localhost:8283

# PostgreSQL configuration
MEMGPT_PG_HOST=localhost
MEMGPT_PG_PORT=5432
MEMGPT_PG_USER=letta
MEMGPT_PG_PASSWORD=letta
MEMGPT_PG_DB=letta
MEMGPT_PG_URI=postgresql://letta:letta@localhost:5432/letta

# Optional: OpenAI API key for embeddings
# OPENAI_API_KEY=your_openai_api_key